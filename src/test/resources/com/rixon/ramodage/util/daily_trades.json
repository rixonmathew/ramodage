{
    "name":"daily_trades",
    "type":"fixed-width",
    "class":"com.rixon.ramodage.strategy.marketdata.TradesGenerationStrategy",
    "fields":[
        {
            "name":"Timestamp",
            "type":"Timestamp",
            "fixedLength":9,
            "minValue":"093000000",
            "maxValue":"170000000",
            "stepValue":"100",
            "formatMask":"HHmmssSSS",
            "padding":"0"
        },
        {
            "name":"Exchange",
            "type":"String",
            "range":"A,B,C,D,I,J,K,M,N,T,P,S,Q,W,X,Y,Z"
        },
        {
            "name":"Symbol",
            "type":"String",
            "range":"IBM             ,MSFT            ,GOOG            ,YHOO            ,GE              "
        },
        {
            "name":"SaleCondition",
            "type":"String",
            "fixedLength":"1",
            "range":"B,S"
        },
        {
            "name":"TradeVolume",
            "type":"BigDecimal",
            "fixedLength":9,
            "minValue":50000,
            "maxValue":60000,
            "padding":0
        },
        {
            "name":"TradePrice",
            "type":"BigDecimal",
            "fixedLength":11,
            "minValue":500000000,
            "maxValue":900000000,
            "padding":0
        },
        {
            "name":"TradeStopStockIndicator",
            "type":"String",
            "range":" ,N,Y"
        },
        {
            "name":"TradeCorectionIndicator",
            "type":"String",
            "range":"00,01,07,08,10,11,12"
        },
        {
            "name":"TradeSequenceNumber",
            "type":"BigDecimal",
            "fixedLength":16,
            "minValue":100000000,
            "maxValue":300000000,
            "padding":0
        },
        {
            "name":"TradeSource",
            "type":"String",
            "range":"00,01,07,08,10,11,12"
        }
    ]
}